<h1>@ViewBag.Title</h1>
<h2>View All</h2>


    foreach (Dictionary<string, string> job in ViewBag.jobs)
    {<table class="job-listing">
       <tr>
        @foreach (KeyValuePair<string, string> item in job)
        {
            <td>
                <a asp-controller="List" asp-action="Values" asp-route-column="@item.Key">@item.Value</a>
            </td>
        }
       </tr>
    }
     </table>
  }